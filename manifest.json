{
    "name": "Baikal",
    "id": "baikal",
    "description": {
        "en": "Lightweight CalDAV+CardDAV server",
        "fr": "Serveur CalDAV+CardDAV l√©ger"
    },
    "developer": {
        "name": "julien",
        "email": "julien.malik@paraiso.me",
        "url": "https://github.com/julienmalik"
    },
    "multi_instance": "true",
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for baikal"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
